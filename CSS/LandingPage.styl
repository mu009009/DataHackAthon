@import "Global.styl";

#MainAttarctText{
	color: DrakButtonTransColor;
	margin-top: 30.00px;
	font-weight: 300;
	margin-bottom: 0px;
	font-size: UtlFontSize;
	line-height: 125%;
}
#SignalsText{
	margin-top: 0px;
	font-weight: lighter;
	line-height: 175%;
	font-size: LargeContentSize;
	margin-bottom: 0px;
	opacity: 0.7;
}
#TitleButtonPart1{
	margin-top: 39.94px;
}
.Introduce{
	text-align: center;
	color: DrakButtonTransColor;
}
.navbar-default .navbar-nav>.open>a, .navbar-default .navbar-nav>.open>a:focus, .navbar-default .navbar-nav>.open>a:hover {
	background-color: navbarButtonBlueHoverOp;
}
.navbar-nav>li>.dropdown-menu{
	background-color: navbarButtonBlueHoverOp;
}
.TimeCountPart{
  background-color: transparent;
  background-image: url(../Resource/LandingPageImages/TitleTimeBoxBack.png);
  background-size: cover;
  border-radius: 5%;
  padding-bottom: 15px;
  padding-top: 15px;
  width: 105px;
  height: 105px;
  margin-bottom: 30px;
}
.TimeTotoalBox{
  width: 560px;
  margin-left: auto;
  margin-right: auto;
  height: auto;
}
.TimeText{
  font-size: UtlFontSize;
  margin-bottom: 5px;
}

.TimeUnitText{
  font-size: MiddleSize;
}

#TitleBar{
	background-color: transparent;
	background: url(../Resource/LandingPageImages/DataHackathonsLandingTitleBackgroundImage.jpg);
	margin-bottom: 0px;
	text-align: center;
	padding-bottom: 94px;
	padding-top: 0px;
	height: auto;
	background-size: cover;
	background-position: center center;
}
#TextSayingPart{
	background-color: DrakButtonTransColor;
	padding-bottom: 114px;
	background: url(../Resource/LandingPageImages/DataHackLandingBottomBackgroundImage.jpg);
	background-size: cover;
}

@media screen and (max-width: 766px) {
	#MainAttarctText{
		font-size: 2.5em;
    margin-top: 0px;
	}
  .TimeTotoalBox{
    width: 280px;
  }
  .TimeCountPart{
    width: 55px;
    height: 55px;
    padding-top: 5px;
    padding-bottom: 5px;
  }
  .TimeText{
    margin-bottom: 6px;
  }
  .jumbotron h1{
    font-size: ThirdSubTitleSize;
  }
  .TimeUnitText{
    font-size: SmallFontSize;
  }
  .TitleTimePartPaddingChange{
    padding-left: 2px;
    padding-right: 2px;
  }
  #Daytext{
    innerHTML: D;
  }
  #Hrtext{
    innerHTML: Hr;
  }
  #Mintext{
    innerHTML: Min;
  }
  #Sectext{
    innerHTML: Sec;
  }
}

