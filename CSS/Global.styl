navbarButtonBlue = rgb(99,106,247);
navbarButtonBlueHoverOp = rgba(navbarButtonBlue,0.8);

navbarButtonHoverColor = rgb(136,134,172);
ButtonBlueHoverColor = rgb(72,168,222);


ColorBlueForText = rgb(162,187,159);

DrakButtonTransColor = rgb(255,255,255);
ShadoWhiteText = rgba(DrakButtonTransColor,0.35);

BlackText = rgb(0,0,0);
ShdowBlack = rgb(153,151,159);

TextBlue = rgb(99,106,247);


BottomCoverColor = rgb(43,43,43);

fontColor = rgb(0,0,0);
fontGrayColor = #666;

shortDurationTime = 0.2s;
MidDurationTime = 1s;

UtlFontSize = 3em;
SubTitleSize = 2em;
ThirdSubTitleSize = 1.3em;

LargeContentSize = 1.2em;
MiddleSize = 1.1em;
NormalFontSize = 1em;
SmallFontSize = 0.875em;
MiniFontSize = 0.5em;

BasicFontSize = 16px;

html{
	height: 100%;
	font: normal 100% Arial, sans-serif;
	overflow-x: hidden;
}
body{
	background-color: DrakButtonTransColor;
	font-family: "Arial";
  font-size: BasicFontSize;
}

.WhiteText{
  transition-duration: shortDurationTime;
	color: DrakButtonTransColor;
}

.BlueText{
  transition-duration: shortDurationTime;
  color: TextBlue;
}

.ImagePhoto{
  width: 100%;
  height: auto;
}

.BoldFont{
  font-weight: bold;
}

.jumbotron h1{
  color: DrakButtonTransColor;
  font-size: UtlFontSize;
  margin-top: 0px;
}

.GrayText{
  transition-duration: shortDurationTime;
  color: fontGrayColor;
}

.GrayText:hover{
  transition-duration: shortDurationTime;
  color: DrakButtonTransColor;
}

.ShadoWhiteContent{
  transition-duration: shortDurationTime;
  color: ShadoWhiteText;
}

.ShdowBlackText{
    transition-duration: shortDurationTime;
    color: ShdowBlack;
}

.ShadoWhiteContent:hover{
  transition-duration: shortDurationTime;
  color: DrakButtonTransColor;
}

.FontLighter{
  font-weight: lighter;
}

.TitleSizeText{
  font-size: UtlFontSize;
}

.SubTitle{
  font-size: SubTitleSize;
}

.ThirdTitle{
  font-size: ThirdSubTitleSize;
}

.LargeContentText{
  font-size: LargeContentSize;
}

.TextTitleMarginBotom{
  margin-bottom: 35px;
}

.LastParaphMarginBottom{
  margin-bottom: 35px;
}

.MiddleSizeText{
  font-size: MiddleSize;
}

.NormalSizeText{
  font-size: NormalFontSize;
}

.MiniSizeText{
  font-size: MiniFontSize;
}

.SmallSizeText{
  font-size: SmallFontSize;
}

.ContentTextTitle{
  font-size: LargeContentSize;
  color: BlackText;
}

.ContentSubTitle{
  font-size: MiddleSize;
  color: fontGrayColor;
  font-weight: bold;
}

.TitleTimePartPaddingChange{
  display: inline-block;
  padding-left: 5px;
  padding-right: 5px;
}

.ContentText{
  font-size: MiddleSize;
  color: fontGrayColor;
}

.SectionBox{
  padding-top: 50px;
  padding-bottom: 50px;
}

.navbar-default .navbar-nav>.open>a, .navbar-default .navbar-nav>.open>a:focus, .navbar-default .navbar-nav>.open>a:hover {
	background-color: navbarButtonHoverColor;
}
.navbar-nav>li>.dropdown-menu{
	background-color: navbarButtonHoverColor;
}
.dropdown-menu>li>a{
	color: DrakButtonTransColor;
}

.nav>li>a:hover{
  background-color: transparent;
}

.site-map {
	background: BottomCoverColor;
	color: fontGrayColor;
}

.site-map-list{
	margin: 50px 0;
	text-align: center;
}
.BottomAlligainChange-Left{
	text-align: left;
}
.BottomAlligainChange-Right{
	text-align: right;
}

.site-map-list a {
	cursor: pointer;
	transition-duration: shortDurationTime;
	color: ShadoWhiteText;
}

.site-map-list a:hover {
	text-decoration: none;
	transition-duration: shortDurationTime;
	color: navbarButtonBlue;
}

/*.site-map-list ul {
	display: inline-block;
}*/

.NavBarButtons{
  color: DrakButtonTransColor;
}

.site-map-list ul li {
	width: 100%;
	text-align: left;
	display: block;
}

.site-map-list ul li.title {
	font-weight: 500;
	padding: 10px 0;
	color: DrakButtonTransColor;
}
.site-map-list title:hover{
	transition-duration: MidDurationTime;
	color: navbarButtonBlue;
}
.site-map-list ul li > a {
	display: inline-block;
	padding: 5px 0;
}

.list-inline li a img{
	transition-duration: shortDurationTime;
	background-color: rgb(5,124,217);
}
.list-inline li a img:hover {
	transition-duration: shortDurationTime;
	background-color: DrakButtonTransColor;
}

.FontAwosomeIcon{

	font-size: NormalFontSize;

	transition-duration: shortDurationTime;
	color: navbarButtonBlue;
}
.FontAwosomeIcon:hover{
	transition-duration: shortDurationTime;
	color: DrakButtonTransColor;
}
.FontIconSize{
	font-size: ThirdSubTitleSize;
	padding-left: 10px;
	padding-right: 10px;
}

#CompanyIconTitleText{
	font-weight: bold;
	font-size: LargeContentSize;
	color: DrakButtonTransColor;
	cursor: pointer;
}
#NavBarPart{
	padding-top: auto;
	padding-bottom: auto;
	background-color: navbarButtonBlue;
	border-color: transparent;
	text-align: center;
	min-height: 70px;
	border-radius: 0%;
}

.navbar-default .navbar-nav>.active>a, .navbar-default .navbar-nav>.active>a:focus, .navbar-default .navbar-nav>.active>a:hover{
  transition-duration: shortDurationTime;
  background-color: inherit;
	border-color: transparent;
	color: DrakButtonTransColor;
	padding-left: 10px;
	padding-right: 10px;
	margin-right: 12px;
}
.navbar-default .navbar-nav>.active>a, .navbar-default .navbar-nav>.active>a:focus, .navbar-default .navbar-nav>.active>a{
  transition-duration: shortDurationTime;
  color: ShadoWhiteText;
}

.navbar-default .navbar-collapse, .navbar-default .navbar-form{
  padding-top: 7px;
  padding-bottom: 7px;
}

#NavbarDetailPart{
	background-color: transparent;
	padding-left: 1%;
	padding-right: 1%;
	float: right;
}
#DraopDownMenu1{
	color: DrakButtonTransColor;
}
.navbar{
	margin-bottom: 0px;
}
#NavTitleCompanyIcon{
	position: absolute;
	margin-top: -10px;
	width: 161.38px;
  height: 50px;
	max-height: 60px;
  background-image: url(../Resource/Icons/Stratifyd/Stratifyd-Logo-Simple-white.png);
  background-size: cover;
  background-position: center center;
}
#FirstTry{
	font-size: LargeContentSize;
	font-weight: 500;
	padding-top: 13.5px;
	height: 60px;
	width: 225px;
	border-radius: 3%;
	transition-duration: shortDurationTime;
	color: DrakButtonTransColor;
	background-color: navbarButtonBlue;
	border-color: navbarButtonBlue;
}
#FirstTry:hover{
	transition-duration: shortDurationTime;
	color: navbarButtonBlue;
	background-color: DrakButtonTransColor;
	border-color: DrakButtonTransColor;
}
#SecondTry{
	font-size: LargeContentSize;
	font-weight: 500;
	padding-top: 13.5px;
	height: 60px;
	width: 225px;
	border-radius: 3%;
	transition-duration: shortDurationTime;
	color: navbarButtonBlue;
	background-color: DrakButtonTransColor;
	border-color: DrakButtonTransColor;
}
#SecondTry:hover{
	transition-duration: shortDurationTime;
	color: DrakButtonTransColor;
	background-color: navbarButtonBlue;
	border-color: navbarButtonBlue;
}
@media screen and (min-width: 992px) {
	.BottomAlligainChange-Left{
		text-align: left;
	}
	.BottomAlligainChange-Right{
		text-align: right;
	}
	.site-map-list {
		margin: 50px 0;
	}
	.site-map-list > div {
		display: inline-block;
	}

	.site-map-list:nth-child(1) {
		text-align: left;
	}

	.site-map-list:nth-child(4) {
		text-align: right;
	}
}

@media screen and (min-width: 766px) and (max-width: 992px){
	.BottomAlligainChange-Left{
		text-align: center;
	}
	.BottomAlligainChange-Right{
		text-align: center;
	}
	.container{
		padding-left: 10%;
		padding-right: 10%;
	}
	.site-map-list {
		margin: 50px 0;
	}
	.site-map-list > div {
		display: inline-block;
	}

	.site-map-list:nth-child(1) {
		text-align: left;
	}

	.site-map-list:nth-child(3) {
		text-align: center;
	}

	.site-map-list:nth-child(2) {
		text-align: center;
	}

	.site-map-list:nth-child(4) {
		text-align: right;
	}
}

@media screen and (max-width: 766px) {
	#NavbarDetailPart{
/*		width: 100%;*/
		margin-left: 0px;
	}
	#FirstTry{
		max-width: 100%;
	}
	.navbar-default .navbar-nav .open .dropdown-menu>li>a{
		color: white;
	}
	.BottomAlligainChange-Left{
		text-align: center;
	}
	.BottomAlligainChange-Right{
		text-align: center;
	}
	.container{
		padding-left: 10%;
		padding-right: 10%;
	}
	#NavBarDropdownMenu{
		text-align: right;
	}
}
